<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ADAltaCafeteriaController">
   <left>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" styleClass="primary-color" stylesheets="@style.css" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/CafeteriaLogo.png" />
               </image>
            </ImageView>
            <Label text="Cafetería UV" textFill="#3d3d3d">
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </left>
   <center>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="120.0" prefWidth="400.0">
               <children>
                  <ImageView fitHeight="80.0" fitWidth="84.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/LogoUV.png" />
                     </image>
                  </ImageView>
                  <Label prefHeight="47.0" prefWidth="183.0" text="Alta de Cafetería" textFill="#3d3d3d">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </VBox>
            <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="70.0" text="Nombre:" textFill="#3d3d3d">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="txfNombreCafeteria" prefHeight="25.0" prefWidth="350.0" stylesheets="@style.css">
                     <styleClass>
                        <String fx:value="input" />
                        <String fx:value="transparent" />
                     </styleClass>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets top="7.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="70.0" text="Facultad:" textFill="#3d3d3d">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox fx:id="cbFacultad" prefHeight="24.0" prefWidth="350.0" stylesheets="@style.css">
                     <styleClass>
                        <String fx:value="radius" />
                        <String fx:value="btn-gris" />
                     </styleClass></ComboBox>
               </children>
               <VBox.margin>
                  <Insets top="7.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="70.0" text="Dirección:" textFill="#3d3d3d">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="txfDireccion" prefHeight="25.0" prefWidth="350.0" stylesheets="@style.css">
                     <styleClass>
                        <String fx:value="transparent" />
                        <String fx:value="input" />
                     </styleClass>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets top="7.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="35.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="120.0" text="Horas de Trabajo:" textFill="#3d3d3d">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets top="7.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="35.0" prefWidth="330.0" spacing="10.0">
               <children>
                  <Label prefHeight="30.0" text="De" textFill="#3d3d3d">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="cbHoraInicio" prefHeight="25.0" prefWidth="24.0" styleClass="btn-gris" stylesheets="@style.css" />
                  <Label prefHeight="30.0" text=":">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="cbMinutoInicio" prefHeight="25.0" prefWidth="14.0" styleClass="btn-gris" stylesheets="@style.css" />
                  <Label prefHeight="30.0" text="a">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="cbHoraFin" prefHeight="25.0" prefWidth="13.0" styleClass="btn-gris" stylesheets="@style.css" />
                  <Label prefHeight="30.0" text=":">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="cbMinutoFin" prefHeight="25.0" prefWidth="0.0" styleClass="btn-gris" stylesheets="@style.css" />
               </children>
               <VBox.margin>
                  <Insets right="25.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="BOTTOM_CENTER" prefHeight="35.0" prefWidth="400.0" spacing="100.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#clicCancelar" stylesheets="@style.css" text="Cancelar" textFill="#3d3d3d">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <styleClass>
                        <String fx:value="shadow" />
                        <String fx:value="btn-gris" />
                     </styleClass>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#clicRegistrar" stylesheets="@style.css" text="Registrar" textFill="#3d3d3d">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <styleClass>
                        <String fx:value="shadow" />
                        <String fx:value="btn-naranja" />
                     </styleClass>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
