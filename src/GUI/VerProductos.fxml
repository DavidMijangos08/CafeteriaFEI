<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="650.0" style="-fx-background-color: #E0E0E0;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.AltaPersonalCafeteriaController">
   <children>
      <Pane prefHeight="40.0" prefWidth="650.0" style="-fx-background-color: #FFFFFF;">
         <children>
            <Label layoutX="14.0" layoutY="2.0" text="Productos">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane layoutX="6.0" layoutY="48.0" prefHeight="353.0" prefWidth="638.0" style="-fx-background-color: #FFFFFF;">
         <children>
            <Button layoutX="440.0" layoutY="314.0" mnemonicParsing="false" onAction="#clicRegresar" text="Regresar" />
            <TableView fx:id="tablaProductos" layoutX="32.0" layoutY="14.0" prefHeight="290.0" prefWidth="575.0">
               <columns>
                  <TableColumn fx:id="columnaNombreProducto" prefWidth="316.0" text="Nombre del producto" />
                  <TableColumn fx:id="columnaPrecio" prefWidth="103.0" text="Precio ($)" />
                  <TableColumn fx:id="columnaTiempoAproximado" prefWidth="155.0" text="Tiempo aproximado (min)" />
               </columns>
            </TableView>
            <Button layoutX="527.0" layoutY="314.0" mnemonicParsing="false" onAction="#clicVer" prefHeight="25.0" prefWidth="70.0" text="Ver" />
         </children>
      </Pane>
   </children>
</AnchorPane>
